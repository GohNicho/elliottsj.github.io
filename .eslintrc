{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "new-cap": [2, {
      "capIsNew": false
    }],
    "quotes": [2, "single"],
    "semi": [2, "never"],
    "strict": [2, "global"]
  }
}
